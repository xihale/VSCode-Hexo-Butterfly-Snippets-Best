{
  "blockquote":{
    "prefix":[">blockquote"],
    "body":[
      "{% blockquote ${1:author} ${2:link} ${3:source_link_title} %}",
      "$4",
      "{% endblockquote %}"
    ],
    "description":"{% blockquote [author[, source]] [link] [source_link_title] %}\ncontent\n{% endblockquote %}"
  },
  "codeblock":{
    "prefix":[">codeblock"],
    "body":[
      "{% codeblock ${1:title} ${2:lang:${3:cpp}} ${4:url} ${5:link text} ${6:mark:$7} %}",
      "$6",
      "{% endcodeblock %}"
    ],
    "description": "{% codeblock [title] [lang:language] [url] [link text] [additional options] %}\ncode snippet\n{% endcodeblock %}"
  },
  "pullquote":{
    "prefix":[">pullquote"],
    "body":[
      "{% pullquote %}",
      "$1",
      "{% endpullquote %}"
    ],
    "description": "Insert pullquote."
  },
  "gist":{
    "prefix":[">gist"],
    "body":[
      "{% gist ${1:gist_id} %}"
    ],
    "description": "Insert gist."
  },
  "iframe":{
    "prefix":[">iframe"],
    "body":[
      "{% iframe ${1:url} $2 %}"
    ],
    "description": "Insert iframe."
  },
  "link":{
    "prefix":[">link"],
    "body":[
      "{% link ${1:text} ${2:url} %}"
    ],
    "description": "Insert link."
  },
